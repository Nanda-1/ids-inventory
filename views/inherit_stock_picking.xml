<?xml version="1.0" encoding="utf-8"?>
<odoo>

    
    <record id="inherit_stock_picking" model="ir.ui.view">
        <field name="name">inherit_stock_picking</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='do_print_picking']" position="replace">
                <button name="print_inherit_report" string="TESSS" type="object"/>
                
            </xpath>
        </field>
    </record>
    

</odoo>
