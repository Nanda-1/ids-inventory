<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="inherit_res_partner_view_form" model="ir.ui.view">
        <field name="name">inherit_res_partner_view_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
                
                <xpath expr="//div[@class='o_address_format']" position="after">
                    <field name="singkatan" placeholder="E.g. PLIN untuk pelindo" />
                </xpath>
                
        </field>  
    </record>

</odoo>
